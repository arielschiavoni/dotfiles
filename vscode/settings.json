{
  "workbench.colorTheme": "One Dark Pro",
  "editor.fontFamily": "JetBrains Mono",
  "editor.fontSize": 10,
  "editor.fontLigatures": true,
  "explorer.openEditors.visible": 0,
  "workbench.iconTheme": "file-icons",
  "files.exclude": {
    "**/.git": true,
    "**/.svn": true,
    "**/.hg": true,
    "**/CVS": true,
    "**/.DS_Store": true,
    "**/node_modules": true,
    "**/*.bs.js": true,
    "**/*.pyc": true,
    "**/.classpath": true,
    "**/.project": true,
    "**/.settings": true,
    "**/.factorypath": true
  },
  "editor.rulers": [100],
  "editor.snippetSuggestions": "top",
  "files.trimTrailingWhitespace": true,
  "files.insertFinalNewline": true,
  "editor.minimap.enabled": false,
  "editor.formatOnSave": true,
  "html.format.enable": false,
  "atomKeymap.promptV3Features": true,
  "editor.multiCursorModifier": "alt",
  "editor.formatOnPaste": false,
  "scss.showErrors": true,
  "php.suggest.basic": false,
  "[php]": {
    "editor.formatOnSave": false
  },
  "python.linting.enabled": false,
  "workbench.editor.enablePreview": false,
  "python.formatting.provider": "yapf",
  "workbench.startupEditor": "newUntitledFile",
  "workbench.editor.enablePreviewFromQuickOpen": false,
  "editor.largeFileOptimizations": false,
  "extensions.ignoreRecommendations": true,
  "prettier.requireConfig": true,
  "eslint.packageManager": "yarn",
  "explorer.confirmDragAndDrop": false,
  "editor.renderWhitespace": "all",
  "javascript.updateImportsOnFileMove.enabled": "never",
  "workbench.editor.showTabs": true,
  "json.schemas": [
    {
      "fileMatch": ["bsconfig.json"],
      "url": "https://raw.githubusercontent.com/BuckleScript/bucklescript/master/docs/docson/build-schema.json"
    }
  ],
  "files.associations": {
    "bsconfig.json": "jsonc"
  },
  "[json]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "[svelte]": {
    "editor.defaultFormatter": "svelte.svelte-vscode"
  },
  "breadcrumbs.enabled": true,
  "typescript.updateImportsOnFileMove.enabled": "always",
  "editor.accessibilitySupport": "off",
  "explorer.confirmDelete": false,
  "[javascript]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "editor.codeActionsOnSave": {
    "source.fixAll": false,
    "source.organizeImports": false
  },
  "peacock.favoriteColors": [
    {
      "name": "Angular Red",
      "value": "#b52e31"
    },
    {
      "name": "Auth0 Orange",
      "value": "#eb5424"
    },
    {
      "name": "Azure Blue",
      "value": "#007fff"
    },
    {
      "name": "C# Purple",
      "value": "#68217A"
    },
    {
      "name": "Gatsby Purple",
      "value": "#639"
    },
    {
      "name": "Go Cyan",
      "value": "#5dc9e2"
    },
    {
      "name": "Java Blue-Gray",
      "value": "#557c9b"
    },
    {
      "name": "JavaScript Yellow",
      "value": "#f9e64f"
    },
    {
      "name": "Mandalorian Blue",
      "value": "#1857a4"
    },
    {
      "name": "Node Green",
      "value": "#215732"
    },
    {
      "name": "React Blue",
      "value": "#00b3e6"
    },
    {
      "name": "Something Different",
      "value": "#832561"
    },
    {
      "name": "Vue Green",
      "value": "#42b883"
    }
  ],
  "editor.suggestSelection": "first",
  "window.menuBarVisibility": "toggle",
  "terminal.integrated.fontSize": 10,
  "python.jediEnabled": false,
  "[jsonc]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "rest-client.fontSize": 10,
  "debug.console.fontSize": 10,
  "markdown.preview.fontSize": 10,
  "[typescript]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "[yaml]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "liveshare.joinDebugSessionOption": "Prompt",
  "dart.flutterSdkPath": "/Users/ariel.schiavoni/repos/franzi/flutter",
  "workbench.preferredDarkColorTheme": "One Dark Pro",
  "[typescriptreact]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "workbench.preferredLightColorTheme": "One Dark Pro",
  "workbench.editorAssociations": {
    "*.ipynb": "jupyter.notebook.ipynb"
  },
  "typescript.tsserver.experimental.enableProjectDiagnostics": false,
  "go.toolsManagement.autoUpdate": true,
  "security.workspace.trust.untrustedFiles": "open",
  "svelte.enable-ts-plugin": true,
  "workbench.activityBar.visible": false,
  "python.languageServer": "Default",
  "[markdown]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "window.zoomLevel": 1,
  "hediet.vscode-drawio.theme": "automatic"
}
